{
  "name": "hails.info",
  "description": "Daniel Hails' personal site",
  "version": "0.1.0",
  "author": "Daniel Hails <daniel@hails.info>",
  "scripts": {
    "start": "gatsby develop",
    "develop": "GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "type": "tsc --noEmit",
    "type:watch": "npm run type -- --watch",
    "lint": "yarn lint:ts && yarn lint:js && yarn lint:md",
    "lint:fix": "yarn lint:ts:fix && yarn lint:js:fix && yarn lint:md:fix",
    "lint:ts": "npx tslint -c tslint.json \"src/**/*.ts?(x)\"",
    "lint:ts:fix": "npx tslint -c tslint.json \"src/**/*.ts?(x)\" --fix",
    "lint:ts:check": "tslint-config-prettier-check ./tslint.json",
    "lint:js": "prettier --ignore-path .gitignore \"*.{js,json}\"",
    "lint:js:fix": "prettier --ignore-path .gitignore --write \"*.{js,json}\"",
    "lint:md": "prettier --ignore-path .gitignore \"src/**/*.md\"",
    "lint:md:fix": "prettier --ignore-path .gitignore --write \"src/**/*.md\""
  },
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@popperjs/core": "^2.4.0",
    "@reach/router": "^1.3.3",
    "@yarnpkg/pnpify": "^2.0.0-rc.22",
    "bootstrap-scss": "^4.4.1",
    "classnames": "^2.2.6",
    "gatsby": "^2.13.28",
    "gatsby-image": "^2.3.2",
    "gatsby-link": "^2.3.3",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-offline": "^3.1.2",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sass": "^2.1.3",
    "gatsby-plugin-sharp": "^2.5.4",
    "gatsby-plugin-typescript": "^2.4.0",
    "gatsby-plugin-typescript-checker": "^1.1.1",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-remark-component": "^1.1.3",
    "gatsby-remark-images": "^3.2.2",
    "gatsby-source-filesystem": "^2.2.1",
    "gatsby-transformer-remark": "^2.7.1",
    "gatsby-transformer-sharp": "^2.4.4",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.15",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-intersection-observer-hook": "^1.0.4",
    "react-popper": "^2.2.3",
    "rehype-react": "^5.0.0",
    "typescript": "^3.5.3",
    "webpack": "^4.36.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/js-cookie": "^2.2.6",
    "@types/lodash": "^4.14.150",
    "@types/node": "^13.13.4",
    "@types/react": "^16.9.35",
    "@types/react-helmet": "^6.0.0",
    "@types/rehype-react": "^4.0.0",
    "gatsby-plugin-tslint": "^0.0.2",
    "prettier": "^2.0.4",
    "sass": "^1.26.5",
    "ts-node": "^8.9.1",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^4.0.0"
  }
}
